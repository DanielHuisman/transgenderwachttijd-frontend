{
    "name": "transgenderwachttijd-frontend",
    "description": "Website which displays the current waiting times for transgender heatlh care in the Netherlands.",
    "version": "1.0.0",
    "license": "UNLICENSED",
    "private": true,
    "contributors": [
        "Daniel Huisman <daniel@huisman.me> (https://danielhuisman.io)"
    ],
    "repository": "github:DanielHuisman/transgenderwachttijd-frontend",
    "keywords": [
        "transgender",
        "trans",
        "wachttijd",
        "waiting time",
        "health care",
        "gezondheidszorg"
    ],
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "scripts": {
        "lint": "eslint src",
        "clean": "rimraf dist",
        "build": "yarn run clean && yarn run lint && yarn run build:parcel",
        "build:parcel": "parcel build public/index.html --public-url / --no-source-maps",
        "dev": "parcel public/index.html",
        "codegen": "dotenv graphql-codegen",
        "codegen:dev": "dotenv graphql-codegen --watch",
        "translate": "i18next -c i18next.config.json"
    },
    "dependencies": {
        "@apollo/client": "^3.5.10",
        "@sentry/react": "^6.19.2",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "@types/react-helmet": "^6.1.5",
        "bootstrap": "^5.1.3",
        "graphql": "^16.3.0",
        "i18next": "^21.6.14",
        "i18next-browser-languagedetector": "^6.1.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.16.2",
        "react-router-dom": "^6.2.2",
        "reactstrap": "^9.0.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/fragment-matcher": "^3.2.1",
